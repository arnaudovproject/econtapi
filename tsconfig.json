{
  "compilerOptions": {
    "outDir": "./dist/lib",
    "rootDir": "./src",
    "strict": true,
    "module": "commonjs",
    "target": "es6",
    "esModuleInterop": true,
    "skipLibCheck": true,
    "forceConsistentCasingInFileNames": true,
    "declaration": true
  },
  "include": ["src/**/*"]
}
